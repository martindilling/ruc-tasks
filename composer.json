{
    "name": "martindilling/ruc-tasks",
    "description": "Interview tasks for a developer job at RUC",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Martin Dilling-Hansen",
            "email": "martindilling@gmail.com",
            "homepage": "https://martindilling.com"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "RucTasks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
